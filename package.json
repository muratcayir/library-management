{
  "name": "library-management",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "classes/*",
    "packages/*"
  ],
  "scripts": {
    "lint": "turbo run lint",
    "build": "turbo run lint && tsc ",
    "format": "prettier-eslint --write 'src/**/*.ts'",
    "test": "turbo run test",
    "prettier": "prettier --write 'classes/**/*.ts'",
    "commitmsg": "commitlint --edit ${1} --verbose",
    "changelog": "git-cliff -o CHANGELOG.md",
    "prepare": "husky install",
    "sort-packages": "npx sort-package-json"
  },
  "dependencies": {
    "axios": "^1.7.7",
    "commitlint": "^19.3.0",
    "connect-redis": "^7.1.1",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-async-errors": "^3.1.1",
    "express-session": "^1.18.0",
    "git-cliff": "^2.4.0",
    "husky": "^8.0.0",
    "prettier": "^3.3.3",
    "redis": "^4.7.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "turbo": "^2.0.9",
    "typescript": "^5.5.4",
    "xss": "^1.0.15",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@stylistic/eslint-plugin-ts": "1.7.0",
    "@types/eslint": "8.56.9",
    "@types/express": "^5.0.0",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.10.1",
    "@types/sequelize": "^4.28.20",
    "@types/xml2js": "^0.4.14",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "eslint": "^8.57.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "18.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.3",
    "eslint-plugin-fp": "2.3.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-n": "17.2.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-unicorn": "^52.0.0",
    "prettier-eslint": "^16.3.0",
    "prettier-eslint-cli": "^8.0.1",
    "rimraf": "^6.0.1",
    "tsconfig-paths": "^4.2.0"
  },
  "packageManager": "npm@7.0.0"
}
